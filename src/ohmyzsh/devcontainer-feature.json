{
  "id": "ohmyzsh",
  "version": "1.1.2",
  "name": "Oh My Zsh",
  "description": "Installs and configures Oh My Zsh in the dev container",
  "options": {
     "theme":{
        "type": "string",
        "enum": ["robbyrussell","af-magic","afowler"],
        "default": "afowler"
     },
     "plugins":{
          "type": "string",
          "default":"vscode vi-mode"    
     },
     "zshrc_append":{
        "type":"string",
        "default":""
     },
     "install_dependencies":{
      "type": "boolean",
      "default":true
     }
  }
  
}
