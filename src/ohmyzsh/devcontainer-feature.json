{
  "id": "ohmyzsh",
  "version": "1.1.0",
  "name": "Oh My Zsh",
  "description": "Installs and configures Oh My Zsh in the dev container",
  "options": {
     "theme":{
        "type": "string",
        "enum": ["test","test2"],
        "default": "test"
     }
  },
  "mounts": [
      {
        "target": "~/.ohmyzsh",
        "source": "~/.ohmyzsh",
        "type": "bind"
      },
      {
        "target": "~/.zshrc",
        "source": "~/.zshrc",
        "type":"bind"
      }
  ]
}
