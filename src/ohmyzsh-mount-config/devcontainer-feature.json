{
  "id": "ohmyzsh-mount-config",
  "version": "1.0.0",
  "name": "Oh My Zsh",
  "description": "Installs and configures Oh My Zsh in the dev container",
  "mounts": [
    {
      "target": "${localEnv:HOME}/.oh-my-zsh/custom/plugins",
      "source": "${localEnv:HOME}/.oh-my-zsh/custom/plugins",
      "type": "bind"
    },
    {
      "target": "${localEnv:HOME}/.zsh_history",
      "source": "${localEnv:HOME}/.zsh_history",
      "type": "bind"
    },
    {
      "target": "${localEnv:HOME}/.zshrc",
      "source": "${localEnv:HOME}/.zshrc",
      "type": "bind"
    }
  ],
  "options": {
   
  }
  
}
